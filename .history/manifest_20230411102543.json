{
    "manifest_version": 3,
    "name": "My Chrome Extension",
    "version": "1.0",
    "description": "A simple Chrome extension to render a floating element",
    "permissions": ["activeTab", "tabs"],
    "content_scripts": [{
      "matches": ["http://localhost:3000/testRoute/index.html"],
      "css": ["style.css"],
      "js": ["content.js"]
    }],
    "action": {
        "default_popup": "popup.html"
      }
  }